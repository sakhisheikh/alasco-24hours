{
    "name": "alasco-24hours",
    "version": "1.0.0",
    "description": "24 Hours javascript challenge",
    "main": "index.js",
    "author": "Sakhi Mansoor",
    "scripts": {
        "start": "webpack-dev-server --mode development --config config/webpack.dev.config.js --open --hot --env.PLATFORM=local --env.VERSION=stag",
        "build": "webpack --mode production --config config/webpack.prod.config.js --env.PLATFORM=production --env.VERSION=stag --progress",
        "build:analyze": "yarn build --env.presets analyze",
        "lint-web": "eslint --debug src/",
        "lint": "eslint --debug src/",
        "lint:write": "eslint --debug src/ --fix",
        "prettier": "prettier --write src/**/*.js",
        "test": "jest",
        "test:watch": "jest --watch",
        "test:coverage": "jest --coverage --colors"
    },
    "husky": {
        "hooks": {
            "pre-commit": "lint-staged && yarn test"
        }
    },
    "lint-staged": {
        "*.(js|jsx)": [
            "yarn lint:write",
            "git add"
        ]
    },
    "license": "ISC",
    "dependencies": {
        "@material-ui/core": "^3.0.0",
        "@material-ui/icons": "^3.0.0",
        "@reach/router": "^1.2.1",
        "axios": "^0.18.0",
        "prop-types": "^15.6.2",
        "react": "^16.6.0",
        "react-dom": "^16.6.0"
    },
    "devDependencies": {
        "@babel/core": "^7.0.0",
        "@babel/plugin-proposal-class-properties": "^7.0.0",
        "@babel/plugin-proposal-export-namespace-from": "^7.0.0",
        "@babel/plugin-proposal-throw-expressions": "^7.0.0",
        "@babel/plugin-syntax-dynamic-import": "^7.0.0",
        "@babel/polyfill": "^7.0.0-beta.51",
        "@babel/preset-env": "^7.0.0-beta.51",
        "@babel/preset-react": "^7.0.0-beta.51",
        "babel-core": "^7.0.0-bridge.0",
        "babel-eslint": "^8.2.3",
        "babel-jest": "^23.4.2",
        "babel-loader": "^8.0.0-beta.0",
        "clean-webpack-plugin": "^0.1.19",
        "copy-webpack-plugin": "^4.5.1",
        "css-hot-loader": "^1.4.2",
        "css-loader": "^0.28.11",
        "enzyme": "^3.3.0",
        "enzyme-adapter-react-16": "^1.1.1",
        "eslint": "^4.19.1",
        "eslint-config-airbnb": "^17.0.0",
        "eslint-config-jest-enzyme": "^6.0.2",
        "eslint-plugin-babel": "^5.1.0",
        "eslint-plugin-import": "^2.12.0",
        "eslint-plugin-jest": "^21.18.0",
        "eslint-plugin-jsx-a11y": "^6.0.3",
        "eslint-plugin-prettier": "^2.6.0",
        "eslint-plugin-react": "^7.9.1",
        "html-webpack-plugin": "^3.2.0",
        "husky": "^1.1.2",
        "jest": "^23.4.2",
        "lint-staged": "^8.0.4",
        "mini-css-extract-plugin": "^0.4.3",
        "node-sass": "^4.8.3",
        "optimize-css-assets-webpack-plugin": "^4.0.0",
        "prettier-eslint": "^8.8.1",
        "react-test-renderer": "^16.4.1",
        "sass-loader": "^7.0.3",
        "style-loader": "^0.21.0",
        "uglifyjs-webpack-plugin": "^1.2.5",
        "webpack": "^4.12.0",
        "webpack-cli": "^3.0.8",
        "webpack-dev-server": "^3.1.4",
        "webpack-merge": "^4.1.3",
        "webpack-visualizer-plugin": "^0.1.11"
    }
}
